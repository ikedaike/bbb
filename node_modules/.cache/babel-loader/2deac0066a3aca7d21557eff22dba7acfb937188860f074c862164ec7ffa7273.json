{"ast":null,"code":"import stampDialog from '../dialog/stampDialog.vue';\nimport dayjs from 'dayjs';\n/* 標準のdateインスタンスは使うのが面倒なのでdayjsを使用 */\nimport 'dayjs/locale/ja';\nexport default {\n  comments: {\n    stampDialog\n  },\n  data() {\n    return {\n      attendanceMark: \"出勤\",\n      leavingMark: \"退勤\",\n      isAttendanceDisable: false,\n      isLeavingDisable: false,\n      nowDate: \"\",\n      nowtime: \"\",\n      attendanceHourMinute: \"\",\n      leavingHourMinute: \"\"\n    };\n  },\n  mounted() {\n    this.nowDate = this.makeMonthDateDayTemplate(this.fetchNowDate());\n    this.nowtime = this.makeHourMinuteTemplate(this.fetchNowDate());\n  },\n  computed: {},\n  methods: {\n    /* 時刻取得 */\n    fetchNowDate() {\n      /* 日本語変換 */\n      dayjs.locale('ja');\n      /* インスタンスを返却 */\n      return dayjs();\n    },\n    /* 時刻表示用フォーマット */\n    makeHourMinuteTemplate(nowDate) {\n      return nowDate.format('HH:mm');\n    },\n    /* ○月○日(○)曜日表示フォーマット */\n    makeMonthDateDayTemplate(nowDate) {\n      return nowDate.format('M月D日(ddd)');\n    },\n    attendanceAtWork() {\n      /* 勤怠ボタンが押下されたら済を表示し、ボタンを非活性 */\n      this.attendanceMark = \"出勤済み\";\n      this.isAttendanceDisable = true;\n      /* 時刻を取得し、表示フォーマットを作成 */\n      this.attendanceHourMinute = this.makeHourMinuteTemplate(this.fetchNowDate());\n    },\n    leavingWork() {\n      this.leavingMark = \"退勤済み\";\n      this.isLeavingDisable = true;\n      this.leavingHourMinute = this.makeHourMinuteTemplate(this.fetchNowDate());\n    },\n    /* 取消が押下された場合 */\n    cancelAttendanceAtWork() {\n      this.attendanceMark = \"出勤\";\n      this.isAttendanceDisable = false;\n      this.attendanceHourMinute = \"\";\n    },\n    cancelLeavingWork() {\n      this.leavingMark = \"退勤\";\n      this.isLeavingDisable = false;\n      this.leavingHourMinute = \"\";\n    }\n  }\n};","map":{"version":3,"mappings":"AA2CA;AACA;AACA;AACA;AAEA;EACAA;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IA