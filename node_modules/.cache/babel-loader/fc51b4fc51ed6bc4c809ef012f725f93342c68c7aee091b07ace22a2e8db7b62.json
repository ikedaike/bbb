{"ast":null,"code":"export default {\n  data() {\n    return {\n      attendanceMark: \"出勤\",\n      leavingMark: \"退勤\",\n      isAttendanceDisable: false,\n      isLeavingDisable: false,\n      nowDate: new Date(),\n      hourMinute: \"\"\n    };\n  },\n  methods: {\n    makeHourMinuteTemplate() {\n      this.hourMinute = this.nowDate.getHours() + \":\" + this.nowDate.getMinutes();\n    },\n    attendanceAtWork() {\n      /* 勤怠ボタンが押下されたら済を表示し、ボタンを非活性 */\n      this.attendanceMark = \"出勤済み\";\n      this.isAttendanceDisable = true;\n      this.makeHourMinuteTemplate();\n    },\n    leavingWork() {\n      this.leavingMark = \"退勤済み\";\n      this.isLeavingDisable = true;\n    },\n    cancelAttendanceAtWork() {\n      this.attendanceMark = \"未\";\n      this.isAttendanceDisable = false;\n      this.hourMinute = \"\";\n    },\n    cancelLeavingWork() {\n      this.leavingMark = \"未\";\n      this.isLeavingDisable = false;\n    }\n  }\n};","map":{"version":3,"mappings":"AAwBA;EACAA;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;MACA;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;EACA;AACA","names":["data","attendanceMark","leavingMark","isAttendanceDisable","isLeavingDisable","nowDate","hourMinute","methods","makeHourMinuteTemplate","attendanceAtWork","leavingWork","cancelAttendanceAtWork","cancelLeavingWork"],"sourceRoot":"src/components/pages","sources":["StampPage.vue"],"sourcesContent":["<template>\n    <div class=\"stamp\">\n        <!-- 出退勤ボタンをクリックしたら非活性にする -->\n        <button @click=\"attendanceAtWork\" :disabled=\"isAttendanceDisable\">出勤</button>\n        <br>\n        <!-- hourMinuteにあたいがあれば時刻を表示 -->\n        {{ attendanceMark }}\n        <div v-show=\"!hourMinute\">\n            未\n        </div>\n        <div v-show=\"hourMinute\">\n            {{ hourMinute }}\n        </div>\n        <!-- 取り消しボタン(bootstrap) -->\n        <button type=\"button\" class=\"btn btn-link\" @click=\"cancelAttendanceAtWork\">[取消]</button>\n        <br>\n        <button @click=\"leavingWork\" :disabled=\"isLeavingDisable\">退勤</button>\n        <br>\n        {{ leavingMark }}\n        <button type=\"button\" class=\"btn btn-link\" @click=\"cancelLeavingWork\">[取消]</button>\n    </div>\n</template>\n    \n<script>\nexport default{\n    data() {\n        return{\n            attendanceMark: \"出勤\",\n            leavingMark: \"退勤\",\n            isAttendanceDisable: false,\n            isLeavingDisable: false,\n            nowDate: new Date(),\n            hourMinute: \"\"\n        }\n    },\n    methods: {\n        makeHourMinuteTemplate(){\n            this.hourMinute = this.nowDate.getHours() + \":\" + this.nowDate.getMinutes()\n        },\n        attendanceAtWork(){\n            /* 勤怠ボタンが押下されたら済を表示し、ボタンを非活性 */\n            this.attendanceMark = \"出勤済み\"\n            this.isAttendanceDisable = true\n            this.makeHourMinuteTemplate()\n        },\n        leavingWork(){\n            this.leavingMark = \"退勤済み\"\n            this.isLeavingDisable = true\n        },\n        cancelAttendanceAtWork(){\n            this.attendanceMark = \"未\"\n            this.isAttendanceDisable = false\n            this.hourMinute = \"\"\n        },\n        cancelLeavingWork(){\n            this.leavingMark = \"未\"\n            this.isLeavingDisable = false\n        },\n    }\n}\n</script>\n    \n<style scoped>\n.cancel-leaving-work{\n    cursor:pointer;\n    display:inline;\n}\n</style>\n    "]},"metadata":{},"sourceType":"module","externalDependencies":[]}